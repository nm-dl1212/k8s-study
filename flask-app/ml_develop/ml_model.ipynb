   "source": [
    "from sklearn.linear_model import LinearRegression\n",
    "import joblib\n",
    "import numpy as np\n",
    "\n",
    "# サンプルデータ\n",
    "X = np.array([[i] for i in range(10)])\n",
    "y = np.array([2 * i + 1 for i in range(10)])\n",
    "\n",
    "# モデルのトレーニング\n",
    "model = LinearRegression()\n",
    "model.fit(X, y)\n",
    "\n",
    "# モデルの保存\n",
    "joblib.dump(model, '../model.joblib')\n"
   ]
  }
 ],
